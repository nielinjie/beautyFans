<manifest
        android:versionName="0.1" android:versionCode="1" package="net.nielinjie.beautyFans"
        xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-sdk android:minSdkVersion="10"></uses-sdk>

    <uses-permission android:name="android.permission.READ_CONTACTS"/>


    <application android:icon="@drawable/android:star_big_on" android:label="@string/app_name">
        <provider android:name="ContentP"
                  android:authorities="net.nielinjie.beautyfans"/>
        <activity android:label="@string/app_name" android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"></action>
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>

    </application>
</manifest>
